    <Resource key="button.style">
        <StyleRuleSet>
            <StyleRule>
                <NoCondition />
                <BorderStyler>
                    <BorderStyle>
                        <Thickness value="1" />
                        <CornerRadius all="2" />
                    </BorderStyle>
                </BorderStyler>
            </StyleRule>
            <StyleRule>
                <ClickStateCondition value="none" />
                <BorderStyler>
                    <BorderStyle>
                        <Color value="#d2d2d2" />
                    </BorderStyle>
                </BorderStyler>
                <CursorStyler>
                    <Cursor value="arrow" />
                </CursorStyler>
            </StyleRule>
            <StyleRule>
                <ClickStateCondition value="hover" />
                <BorderStyler>
                    <BorderStyle>
                        <Color value="#0078d4" />
                    </BorderStyle>
                </BorderStyler>
                <CursorStyler>
                    <Cursor value="hand" />
                </CursorStyler>
            </StyleRule>
            <StyleRule>
                <ClickStateCondition value="press" />
                <BorderStyler>
                    <BorderStyle>
                        <Color value="#06579b" />
                    </BorderStyle>
                </BorderStyler>
                <CursorStyler>
                    <Cursor value="hand" />
                </CursorStyler>
            </StyleRule>
            <StyleRule>
                <ClickStateCondition value="pressinactive" />
                <BorderStyler>
                    <BorderStyle>
                        <Color value="#06579b" />
                    </BorderStyle>
                </BorderStyler>
                <CursorStyler>
                    <Cursor value="arrow" />
                </CursorStyler>
            </StyleRule>
        </StyleRuleSet>
    </Resource>

